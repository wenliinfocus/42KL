Files to turn in : ft_strupcase.c

/* Create a function that transforms every letter to uppercase.
  */

#include <unistd.h>

char *ft_startupcase(char *str)
{
  int i;

  i = 0;
  while (str[i] != '\0')
  {
    if (str[i] >= 'a' && str[i] <= 'z')
    {
      str[i] = str[i] - 32;
    }
    i++;
  }
  return (str);
}
