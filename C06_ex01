Files to turn in : ft_print_params.c

/*
A program that prints its input one word per line.
*/

#include <unistd.h>

int  main(int argc, char *argv[])
{
  int  i;
  int  count;

  count = 0;
  i = -1;
  while (++count < argc)
  {
    while (argv[count][i++] != '\0')
    {
      write (1, &argv[count][i], 1);
    }
    write (1, "\n", 1);
    i = -1;
  }
}
