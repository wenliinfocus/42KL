Files to turn in: ft_strstr.c

/* The strstr() function locates the first occurrence of the null-terminated string s2 in the null-terminated string s1.
*/

#include <unistd.h>

char  *ft_strstr(char *str, char *to_find)
{
  unsigned int  i;
  unsigned int  k;

  i = 0;
  if (to_find[0] == '\0')
  {
    return (str);
  }
  while (str[i] != '\0')
  {
    k = 0;
    while (str[i + k] == to_find[k])
    {
      if (to_find[k + 1] == '\0')
      {
        return (str + i);
      }
      k++;
    }
    i++;
  }
  return (0);
}
